<div class="container m-5">
  <h1>Registered products table</h1>
  <table class="table table-primary table-striped">
    <thead>
        <tr>
        <th scope="col">Product name</th>
        <th scope="col">Price</th>
        <th scope="col">Thumbnail</th>
        </tr>
    </thead>
    <tbody>
        <% products.forEach(function(prod){ %>
          <tr>
              <td class="prodTitle"><%= prod.title %></td>
              <td class="prodPrice"><%= prod.price %></td>
              <td><img alt="ðŸ“·" class="prodThumbnail" src=<%= prod.thumbnail %>></td>
          </tr>
        <% }) %>
    </tbody>
  </table>
  <a href="/" class="btn btn-lg btn-info" role="button"><i class="bi-house-fill"></i>Home</a>
</div>